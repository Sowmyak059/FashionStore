@model IEnumerable<Product>

<h2>Your Wishlist</h2>
<div class="wishlist">
    @foreach (var product in Model)
    {
        <div class="wishlist-item">
            <img src="@product.ImageUrl" alt="@product.Name" />
            <h3>@product.Name</h3>
            <p>@product.Description</p>
            <p>@product.Price</p>
        </div>
    }
</div>